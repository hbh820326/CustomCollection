<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HeadView">
        <attr name="textColor" format = "color"/>
        <attr name="HVtextSize" format = "dimension"/>
        <attr name="HViconSize" format = "dimension"/>
        <attr name="space" format = "dimension"/>
        <attr name="centreIcon" format = "reference"/>
        <attr name="centreString" format = "string"/>
        <attr name="leftString" format = "string"/>
        <attr name="leftIcon" format = "reference"/>
        <attr name="rightString" format = "string"/>
        <attr name="rightIcon" format = "reference"/>
    </declare-styleable>

    <declare-styleable name="ZcView">
        <attr name="iconSize" format="dimension" />
        <attr name="textTop" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor1" format="color" />
        <attr name="textColor2" format="color" />
        <attr name="radius" format="dimension" />

    </declare-styleable>
    <declare-styleable name="GonGeView">
        <attr name="GViconSize" format="dimension" />
        <attr name="GVtextTop" format="dimension" />
        <attr name="GVtextSize" format="dimension" />
        <attr name="GVradius" format="dimension" />
        <attr name="GVcolumn" format="integer" />
        <attr name="GVcellHeight" format="dimension" />

    </declare-styleable>

    <declare-styleable name="SwichView">
        <attr name="svich_radius" format="dimension" />
        <attr name="thickness" format="dimension" />
        <attr name="openColor" format="color" />
        <attr name="closeColor" format="color" />
    </declare-styleable>
    <declare-styleable name="LetterIndexView">
        <attr name="textsize" format="dimension"/>
        <attr name="cellwidth" format="dimension"/>
        <attr name="markwidth" format="dimension"/>
        <attr name="mark_text_size" format="dimension"/>

    </declare-styleable>
</resources>
